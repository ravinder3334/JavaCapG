package com.capg.work;

import java.util.Scanner;

public class MyArray {
	public static void main(String[] args) {
		
		float arr[] = new float[5];
		Scanner sc = new Scanner(System.in);
		for(int i=0; i<arr.length;i++) 
		{
			arr[i]=sc.nextFloat();
		}
		for(float a: arr) {
			System.out.println(a);
		}
 	}

}
