package com.capg.work;

public class MyThrow {

	// throws 
	public static void grade(int marks) throws Exception {
		if(marks<33) {
			throw new Exception("Work harder");
		}
		else {
			System.out.println("Doing good");
		}
	}
	public static void main(String[] args) throws Exception{
//		MyThrow.grade(40);
		grade(12);
	}
}
